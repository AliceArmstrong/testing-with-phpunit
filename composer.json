{
    "scripts": {
        "test-original": "./vendor/bin/phpunit",
        "test": "./vendor/bin/phpunit --testdox --colors=always",
        "test-runtime": "./vendor/bin/phpunit --testdox --verbose --colors=always",
        "coverage": "./vendor/bin/phpunit --coverage-text=tests"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "require": {
        "laravel/laravel": "^8.4"
    }
}
